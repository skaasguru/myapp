<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>My App</display-name>
  <servlet>
  	<servlet-name>Contactservlet</servlet-name>
  	<display-name>Contactservlet</display-name>
  	<description></description>
  	<servlet-class>com.skaas.webapp.Contactservlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Galleryservlet</servlet-name>
  	<display-name>Galleryservlet</display-name>
  	<description></description>
  	<servlet-class>com.skaas.webapp.Galleryservlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Loginservlet</servlet-name>
  	<display-name>Loginservlet</display-name>
  	<description></description>
  	<servlet-class>com.skaas.webapp.Loginservlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Signupservlet</servlet-name>
  	<display-name>Signupservlet</display-name>
  	<description></description>
  	<servlet-class>com.skaas.webapp.Signupservlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Contactservlet</servlet-name>
  	<url-pattern>/contactservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Galleryservlet</servlet-name>
  	<url-pattern>/galleryservlet/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Loginservlet</servlet-name>
  	<url-pattern>/loginservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Signupservlet</servlet-name>
  	<url-pattern>/signupservlet</url-pattern>
  </servlet-mapping>
</web-app>
